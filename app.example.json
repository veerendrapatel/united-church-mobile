{
  "expo": {
    "name": "<your app name>",
    "slug": "<your app slug>",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "<com.your-domain.your-app-name>",
      "config": {
        "googleSignIn": {
          "reservedClientId": "<your client id>"
        }
      }
    },
    "android": {
      "package": "<com.your_domain.your_appname>",
      "config": {
        "googleSignIn": {
          "apiKey": "<your api key>",
          "certificateHash": "<your certificate hash>"
        }
      }
    },
    "githubUrl": "https://github.com/sopherapps/united-church-mobile",
    "primaryColor": "#485e88",
    "scheme": "<a scheme of your choice>"
  }
}
